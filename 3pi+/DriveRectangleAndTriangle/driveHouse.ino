void driveHouse(int pSpeed) {
  driveDistance(200, pSpeed);
  turn(45);
  driveDistance(141.4, pSpeed);
  turn(90);
  driveDistance(141.4, pSpeed);
  turn(45);
  driveDistance(200, pSpeed);
  turn(135);
  driveDistance(282.8, pSpeed);
  turn(135);
  driveDistance(200, pSpeed);
  turn(135);
  driveDistance(282.8, pSpeed);
  turn(-135);
  driveDistance(200, pSpeed);
  turn(-90);
}